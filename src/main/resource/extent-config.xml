<?xml version="1.0" encoding="UTF-8"?>

<extentreports>
	<configuration>
		
		<!-- report theme--> <!-- standard,dark-->
		<theme>standard</theme>
		
		<!-- documnet encoding--> <!-- defaults to UTF-8-->
		<protocol>https</protocol>
		
		<!-- title of document-->
		 <documentTitle>Title -  Cucumber report</documentTitle>
		 
		 <!-- report name -displayed at top of nav-->
		 <reportName>Report name - Cucucmber Report</reportName>
		 
		 <!-- global date format override--><!-- defaults to yyyy-MM-dd-->
		<dateFormat>yyyy-MM-dd</dateFormat>
		
		<!-- global time format override--><!-- defaults to HH:mm:ss-->
		<timeFormat>HH:mm:ss</timeFormat>
		
		<!-- custom javascript-->
		<scripts>
			<![CDATA[ 
			$(document).ready(function(){
			
			});
			]]>
		</scripts>
		
	<!-- custom styles-->
		<styles>
			<![CDATA[ 
			
			]]>
		</styles>
	</configuration>
</extentreports>